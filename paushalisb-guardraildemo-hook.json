{
    "typeName": "paushalisb::guardraildemo::hook",
    "description": "S3 Versioning verification hook",
    "sourceUrl": "https://github.com/PaushaliKunduTR/proactive-gov-guardrails",
    "documentationUrl": "https://github.com/PaushaliKunduTR/proactive-gov-guardrails/blob/main/README.md",
    "typeConfiguration": {
        "properties": {
            "excludedBuckets": {
                "description": "Exceptions for S3 Buckets that should be excluded from Versioning Requirements",
                "type": "string"
            }
        },
        "additionalProperties": false
    },
    "required": [],
    "handlers": {
        "preCreate": {
            "targetNames": [
                "AWS::S3::Bucket"
            ],
            "permissions": []
        },
        "preUpdate": {
            "targetNames": [
                "AWS::S3::Bucket"
            ],
            "permissions": []
        }
    },
    "additionalProperties": false
}
